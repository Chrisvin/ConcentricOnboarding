<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConcentricOnboardingViewPager">
        <attr name="scrollerDuration" format="integer" />
        <attr name="mode" format="enum">
            <enum name="slide" value="0" />
            <enum name="reveal" value="1" />
        </attr>
        <attr name="scaleXFactor" format="float" />
        <attr name="scaleYFactor" format="float" />
        <attr name="translationXFactor" format="float" />
        <attr name="translationYFactor" format="float" />
    </declare-styleable>
</resources>